<?xml version="1.0" encoding="UTF-8"?>
<MT526>
    <Sequence name="A" description="Mandatory Sequence A" opt="M" repeat="false">
        <Tag no="1" name="20" opt="M" format="16x" regexp="^[${x}]{1,16}$">
            <content type="string" maxlength="16"/>
        </Tag>
        <Tag no="2" name="21" opt="O" format="16x" regexp="^[${x}]{1,16}$">
            <content type="string" maxlength="16"/>
        </Tag>
        <Tag no="3" name="23" opt="M" format="6!a[/4!c]" regexp="^[${a}]{6}(\/[${c}]{4})?$">
            <content type="enum-data" datafor="Code">
                {
                 'BORROW':['CANC' ,'HOLD' ]
                ,'NOLEND':[]
                ,'RECALL':[ 'COMP' ,'PART' ]
                ,'RETURN':[ 'COMP' ,'PART' ]
                ,'TOLEND':[ 'FILL' ,'HELD' ]
                }
            </content>
            <content type="enum" contentName="Code" prefix="/"/>
        </Tag>
        <Tag no="4" name="83C" opt="O" format="/34x" regexp="^\/[${x}]{1,34}$">
            <content type="fval">/</content>
            <content type="string" maxlength="34"/>
        </Tag>
        <Tag no="5" name="29A" opt="O" format="4*35x" regexp="^([${x}]{1,35}\r\n){0,3}([${x}]{1,35}){1}$">
            <content type="textarea" cols="35" rows="4"/>
        </Tag>
        <Tag no="6" name="29B" opt="O" format="4*35x" regexp="^([${x}]{1,35}\r\n){0,3}([${x}]{1,35}){1}$">
            <content type="textarea" cols="35" rows="4"/>
        </Tag>
    </Sequence>
    <Sequence name="B" description="Mandatory Repetitive Sequence B" opt="M" repeat="true">
        <Tag no="7" name="35B" opt="M" format="[ISIN1!e12!c][4*35x]" regexp="^(ISIN [${c}]{12})?\r\n(([${x}]{1,35}\r\n){0,3}([${x}]{1,35}){1})?$">
            <content type="string" fixedlength="17"/>
            <content type="textarea" cols="35" rows="4" prefix="lf"/>
        </Tag>
        <Tag no="8" name="35A" opt="O" format="3!a15d" regexp="^[${a}]{3}${d}$">
            <content type="enum">
                [ 'BON' ,'CER' ,'CPN' ,'FMT' ,'MSC' ,'OPC' ,'OPS' ,'PRC' ,'PRS' ,'RTE' ,'RTS' ,'SHS' ,'UNT' ,'WTS' ]
            </content>
            <content type="decimal" maxlength="15"/>
        </Tag>
        <Tag no="9" name="30" opt="M" format="6!n" regexp="^[${n}]{6}$">
            <content type="date" format="yyMMdd"/>
        </Tag>
        <Tag no="10" name="31F" opt="O" format="6!n[/6!n][//35x]" regexp="^[${n}]{6}([${n}]{6})?(\/\/[${x}]{1,35})$">
            <content type="date" format="yyMMdd"/>
            <content type="date" format="yyMMdd" prefix="/"/>
            <content type="string" maxlength="35" prefix="//"/>
        </Tag>
        <Tag no="11" name="87a" opt="O">
            <option>
                <Tag name="87A" format="[/1!a][/34x]4!a2!a2!c[3!c]" regexp="^(\/[${a}]{1})?(\/[${x}]{1,34})?[${a}]{4}[${a}]{2}[${c}]{2}([${c}]{3})?">
                        <content type="string" maxlength="1" prefix="/"/>
                        <content type="string" maxlength="34" prefix="/"/>
                        <content type="IdentifierCode"/>
                </Tag>
                <Tag name="87D" format="[/1!a][/34x]4*35x" regexp="^(\/[${a}]{1})?(\/[${x}]{1,34})?([${x}]{1,35}\r\n){0,3}([${x}]{1,35}){1}">
                        <content type="string" maxlength="1" prefix="/"/>
                        <content type="string" maxlength="34" prefix="/"/>
                        <content type="textarea" cols="35" rows="4"/>
                </Tag>
            </option>
        </Tag>
        <Tag no="12" name="72" opt="O" format="6*35x" regexp="^([${x}]{1,35}\r\n){0,5}([${x}]{1,35}){1}$">
            <content type="textarea" cols="35" rows="6"/>
        </Tag>
    </Sequence>
</MT526>