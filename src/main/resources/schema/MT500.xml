<?xml version="1.0" encoding="UTF-8"?>
<MT500>
    <Sequence name="A" description="必填分页A基本信息" opt="M" repeat="false">
        <Tag name="16R" description="Start of Block" format="GENL" opt="M">
            <content type="fval">GENL</content>
        </Tag>
        <Tag name="20C" description="Sender's Message Reference" opt="M">
            <content type="fval">:</content>
            <content type="enum">['SEME']</content>
            <content type="fval">//</content>
            <content type="string" maxlength="16" characterSet="x"></content>
        </Tag>
        <Tag name="23G" description="Function of the Message" opt="M">
            <content type="enum">['CANC','NEWM']</content>
            <content type="enum">['//CODU','//COPY','//DUPL']</content>
        </Tag>
        <Tag name="98a" description="Function of the Message" opt="O">
            <option>
                <Tag name="98A">
                    <content type="fval">:</content>
                    <content type="enum">['PREP']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
                <Tag name="98C">
                    <content type="fval">:</content>
                    <content type="enum">['PREP']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
                <Tag name="98E">
                    <content type="fval">:</content>
                    <content type="enum">['PREP']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
            </option>
        </Tag>
        <Tag name="22F" opt="M">
            <content type="fval">:</content>
            <content type="enum">['INST']</content>
            <content type="fval">/</content>
            <content type="string" maxlength="8"></content>
            <content type="fval">/</content>
            <content type="enum">['DERI','REGI','RERE']</content>
        </Tag>
        <SubSequence name="A1" description="必填分页A基本信息" opt="O" repeat="true">
            <Tag name="16R" opt="M">
                <content type="fval">LINK</content>
            </Tag>
            <Tag name="22F" opt="O">
                <content type="fval">:</content>
                <content type="enum">['LINK']</content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum">['AFTE','BEFO','INFO','WITH']</content>
            </Tag>
            <Tag name="13a" opt="O">
                <option>
                    <Tag name="13A">
                        <content type="fval">:</content>
                        <content type="enum">['LINK']</content>
                        <content type="fval">//</content>
                        <content type="string" maxlength="3"></content>
                    </Tag>
                    <Tag name="13B">
                        <content type="fval">:</content>
                        <content type="enum">['LINK']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="20C" opt="M">
                <content type="fval">:</content>
                <content type="enum">['POOL','PREV','RELA']</content>
                <content type="fval">//</content>
                <content type="string" maxlength="16"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">LINK</content>
            </Tag>
        </SubSequence>
        <Tag name="16S" opt="M">
            <content type="fval">GENL</content>
        </Tag>
    </Sequence>

    <Sequence name="B" description="必填分页B Registration Details" opt="M" repeat="false">
        <Tag name="16R" opt="M">
            <content type="fval">REGDET</content>
        </Tag>
        <Tag name="20D" opt="M" repeat="true">
            <content type="fval">:</content>
            <content type="enum">['BREF','BPAR','SHAR']</content>
            <content type="fval">//</content>
            <content type="string" maxlength="25"></content>
        </Tag>
        <Tag name="22F" opt="M" repeat="true">
            <content type="fval">:</content>
            <content type="enum-data" datafor="Indicator">
                {'OWNT':['NOMI','OWNE','TRUS']
                ,'BENT':['INDV','LEGL']
                ,'ADMT':['GIVE','NGIV','TIME']
                ,'RREA':['BEAR','BONU','CAPG','DVCA','EXWA','INCR','INHE','OTHR','POSS','REGD','STAF']
                ,'CERT':['NTBK','TOBK']}
            </content>
            <content type="fval">/</content>
            <content type="string" maxlength="8"></content>
            <content type="fval">/</content>
            <content type="enum" contentName="Indicator"></content>
        </Tag>
        <Tag name="98a" opt="O">
            <option>
                <Tag name="98A">
                    <content type="fval">:</content>
                    <content type="enum">['RREG']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
                <Tag name="98C">
                    <content type="fval">:</content>
                    <content type="enum">['RREG']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
            </option>
        </Tag>
        <Tag name="35B" opt="M">
            <content type="string" maxlength="17"></content>
            <content type="textarea" cols="35" rows="4"></content>
        </Tag>
        <Tag name="70C" opt="O">
            <content type="fval">:</content>
            <content type="enum">['REGI']</content>
            <content type="fval">//</content>
            <content type="textarea" cols="35" rows="4"></content>
        </Tag>
        <SubSequence name="B1" description="必填分页A基本信息" opt="O" repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">FIA</content>
            </Tag>
            <Tag name="94B" opt="O">
                <content type="fval">:</content>
                <content type="enum">['PLIS']</content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum">['EXCH','OTCO']</content>
                <content type="string" maxlength="30" prefix="/"></content>
            </Tag>
            <Tag name="22F" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum-data" datafor="Indicator">
                    {'MICO':['A001','A002','A003','A004','A005','A006','A007','A008','A009','A010','A011','A012','A013','A014','OTHR']
                    ,'FORM':['BEAR','REGD']
                    ,'PFRE':['ANNU','MNTH','QUTR','SEMI','WEEK']
                    ,'PAYS':['FULL','NILL','PART']
                    ,'REST':['144A','NRST','RSTR']}
                </content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum" contentName="Indicator"></content>
            </Tag>
            <Tag name="12a" opt="O" repeat="true">
                <option>
                    <Tag name="12A">
                        <content type="fval">:</content>
                        <content type="enum">['OPST','OPTI']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30" ></content>
                    </Tag>
                    <Tag name="12B">
                        <content type="fval">:</content>
                        <content type="enum-data" datafor="Indicator">
                            {'OPST':['AMER','EURO']
                            ,'OPTI':['CALL','PUTO']}
                        </content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="enum" contentName="Indicator"></content>
                    </Tag>
                    <Tag name="12C">
                        <content type="fval">:</content>
                        <content type="enum">['OPST','OPTI']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="6" ></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="11A" opt="O">
                <content type="fval">:</content>
                <content type="enum">['DENO']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="3" ></content>
            </Tag>
            <Tag name="98A" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['COUP','EXPI','FRNR','MATU','ISSU','CALD','PUTT','DDTE','FCOU']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="8" ></content>
            </Tag>
            <Tag name="92A" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['PRFC','CUFC','NWFC','INTR','NXRT','YTMR']</content>
                <content type="fval">//</content>
                <content type="enum">['N']</content>
                <content type="string" maxlength="15" ></content>
            </Tag>
            <Tag name="13a" opt="O" repeat="true">
                <option>
                    <Tag name="13A">
                        <content type="fval">:</content>
                        <content type="enum">['COUP']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="3"></content>
                    </Tag>
                    <Tag name="13B">
                        <content type="fval">:</content>
                        <content type="enum">['COUP','POOL','LOTS']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="17B" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['FRNF','CALL','PUTT']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="1"></content>
            </Tag>
            <Tag name="90a" opt="O" repeat="true">
                <option>
                    <Tag name="90A">
                        <content type="fval">:</content>
                        <content type="enum">['INDC','MRKT','EXER']</content>
                        <content type="fval">//</content>
                        <content type="enum">['DISC','PRCT','PREM','YIEL']</content>
                        <content type="enum">['N']</content>
                        <content type="string" maxlength="15"></content>
                    </Tag>
                    <Tag name="90B">
                        <content type="fval">:</content>
                        <content type="enum">['INDC','MRKT','EXER']</content>
                        <content type="fval">//</content>
                        <content type="enum">['ACTU','DISC','PREM']</content>
                        <content type="string" fixedlength="3"></content>
                        <content type="string" maxlength="15"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="36B" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['MINO','SIZE']</content>
                <content type="fval">//</content>
                <content type="enum">['AMOR','FAMT','UNIT']</content>
                <content type="string" maxlength="15"></content>
            </Tag>
            <Tag name="70E" opt="O">
                <content type="fval">:</content>
                <content type="enum">['FIAN']</content>
                <content type="fval">//</content>
                <content type="textarea" cols="35" rows="10"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">FIA</content>
            </Tag>
        </SubSequence>
        <SubSequence name="B2" description="Mandatory Subsequence B2 Financial Instrument/Account" opt="M"
                     repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">FIA</content>
            </Tag>
            <Tag name="36B" opt="M">
                <content type="fval">:</content>
                <content type="enum">['QREG']</content>
                <content type="fval">//</content>
                <content type="enum">['AMOR','FAMT','UNIT']</content>
                <content type="string" maxlength="15"></content>
            </Tag>
            <Tag name="95a" opt="O">
                <option>
                    <Tag name="95P">
                        <content type="fval">:</content>
                        <content type="enum">['ACOW']</content>
                        <content type="fval">//</content>
                        <content type="string" maxlength="11"></content>
                    </Tag>
                    <Tag name="95R">
                        <content type="fval">:</content>
                        <content type="enum">['ACOW']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="34"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="97A" opt="M">
                <content type="fval">:</content>
                <content type="enum">['SAFE']</content>
                <content type="fval">//</content>
                <content type="string" maxlength="35"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">FIAC</content>
            </Tag>
        </SubSequence>
        <Tag name="16S" opt="M">
            <content type="fval">REGDET</content>
        </Tag>
    </Sequence>
    <Sequence name="C" description="Optional Repetitive Sequence C Client Details" opt="O" repeat="true">
        <Tag name="16R" opt="M">
            <content type="fval">CLTDET</content>

        </Tag>
        <SubSequence name="C1" description="Optional Subsequence C1 Address" opt="O" repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">ADDRESS</content>
            </Tag>
            <Tag name="17B" opt="M"></Tag>
            <Tag name="22F" opt="O" repeat="true"></Tag>
            <Tag name="95a" opt="O" repeat="true">
                <option>
                    <Tag name="95P"></Tag>
                    <Tag name="95R"></Tag>
                    <Tag name="95S"></Tag>
                    <Tag name="95U"></Tag>
                </option>
            </Tag>
            <Tag name="94a" opt="O" repeat="true">
                <option>
                    <Tag name="94C"></Tag>
                    <Tag name="94D"></Tag>
                    <Tag name="94G"></Tag>
                </option>
            </Tag>
            <Tag name="13B" opt="O" repeat="true"></Tag>
            <Tag name="70C" opt="O" repeat="true"></Tag>
            <Tag name="16S" opt="M"></Tag>
        </SubSequence>
        <SubSequence name="C2" description="Optional Subsequence C2 Personal Details" opt="O" repeat="false">
            <Tag name="16R" opt="M"></Tag>
            <Tag name="22F" opt="O" repeat="true"></Tag>
            <Tag name="95U" opt="O" ></Tag>
            <Tag name="98a" opt="O" repeat="true">
                <option>
                    <Tag name="98A" ></Tag>
                    <Tag name="98C" ></Tag>
                </option>
            </Tag>
            <Tag name="94C" opt="O" repeat="true"></Tag>
            <Tag name="70C" opt="O"></Tag>
            <Tag name="16S" opt="M"></Tag>
        </SubSequence>
        <Tag name="16S" opt="M"></Tag>
    </Sequence>
    <Sequence name="D" description="Optional Sequence D Corporate Action Details" opt="O" >
        <Tag name="16R" opt="M"></Tag>
        <Tag name="98a" opt="O" repeat="true">
            <option>
                <Tag name="98A" ></Tag>
                <Tag name="98C" ></Tag>
            </option>
        </Tag>
        <Tag name="35B" opt="O"></Tag>
        <Tag name="16S" opt="M"></Tag>
    </Sequence>
    <Sequence name="E" description="Optional Sequence E Additional Information" opt="O" >
        <Tag name="16R" opt="M"></Tag>
        <Tag name="95a" opt="O" repeat="true">
            <option>
                <Tag name="95P" ></Tag>
                <Tag name="95Q" ></Tag>
                <Tag name="95R" ></Tag>
            </option>
        </Tag>
        <Tag name="16S" opt="M"></Tag>
    </Sequence>
</MT500>     