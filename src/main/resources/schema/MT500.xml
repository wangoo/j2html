<?xml version="1.0" encoding="UTF-8"?>
<MT500>
    <Sequence name="A" description="Mandatory Sequence A General Information" opt="M" repeat="false">
        <Tag name="16R" description="Start of Block"  opt="M">
            <content type="fval">GENL</content>
        </Tag>
        <Tag name="20C" description="Sender's Message Reference" opt="M" format=":4!c//16x" regexp="^:[${c}]{4}\/\/[${x}]{1,16}$">
            <content type="fval">:</content>
            <content type="enum">['SEME']</content>
            <content type="fval">//</content>
            <content type="string" maxlength="16" characterSet="x"></content>
        </Tag>
        <Tag name="23G" description="Function of the Message" opt="M" format="4!c[/4!c]" regexp="^[${c}]{4}(\/[${c}]{4})?$">
            <content type="enum">['CANC','NEWM']</content>
            <content type="enum">['/CODU','/COPY','/DUPL']</content>
        </Tag>
        <Tag name="98a" description="Function of the Message" opt="O">
            <option>
                <Tag name="98A" format=":4!c//8!n" regexp="^:[${c}]{4}\/\/[${n}]{8}$">
                    <content type="fval">:</content>
                    <content type="enum">['PREP']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
                <Tag name="98C" format=":4!c//8!n6!n" regexp="^:[${c}]{4}\/\/[${n}]{8}[${n}]{6}$">
                    <content type="fval">:</content>
                    <content type="enum">['PREP']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
                <Tag name="98E">
                    <content type="fval">:</content>
                    <content type="enum">['PREP']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
            </option>
        </Tag>
        <Tag name="22F" opt="M" format=":4!c/[8c]/4!c" regexp="^:[${c}]{4}\/([${c}]{0,8})?\/[${c}]{4}$">
            <content type="fval">:</content>
            <content type="enum">['INST']</content>
            <content type="fval">/</content>
            <content type="string" maxlength="8" characterSet="c"></content>
            <content type="fval">/</content>
            <content type="enum">['DERI','REGI','RERE']</content>
        </Tag>
        <SubSequence name="A1" description="Optional Repetitive Subsequence A1 Linkages" opt="O" repeat="true">
            <Tag name="16R" opt="M">
                <content type="fval">LINK</content>
            </Tag>
            <Tag name="22F" opt="O" format=":4!c/[8c]/4!c" regexp="^:[${c}]{4}\/([${c}]{0,8})?\/[${c}]{4}$">
                <content type="fval">:</content>
                <content type="enum">['LINK']</content>
                <content type="fval">/</content>
                <content type="string" maxlength="8" characterSet="c"></content>
                <content type="fval">/</content>
                <content type="enum">['AFTE','BEFO','INFO','WITH']</content>
            </Tag>
            <Tag name="13a" opt="O">
                <option>
                    <Tag name="13A" format=":4!c//3!c" regexp="^:[${c}]{4}\/\/[${c}]{3}$">
                        <content type="fval">:</content>
                        <content type="enum">['LINK']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="3" characterSet="c"></content>
                    </Tag>
                    <Tag name="13B" format=":4!c/[8c]/30x" regexp="^:[${c}]{4}\/([${c}]{0,8})?\/[${x}]{1,30}$">
                        <content type="fval">:</content>
                        <content type="enum">['LINK']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8" characterSet="c"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30" characterSet="x"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="20C" opt="M"  format=":4!c//16x" regexp="^:[${c}]{4}\/\/[${x}]{1,16}$">
                <content type="fval">:</content>
                <content type="enum">['POOL','PREV','RELA']</content>
                <content type="fval">//</content>
                <content type="string" maxlength="16" characterSet="x"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">LINK</content>
            </Tag>
        </SubSequence>
        <Tag name="16S" opt="M">
            <content type="fval">GENL</content>
        </Tag>
    </Sequence>
    <Sequence name="B" description="Mandatory Sequence B Registration Details" opt="M" repeat="false">
        <Tag name="16R" opt="M">
            <content type="fval">REGDET</content>
        </Tag>
        <Tag name="20D" opt="M" repeat="true" format=":4!c//25x" regexp="^:[${c}]{4}\/\/[${x}]{1,25}$">
            <content type="fval">:</content>
            <content type="enum">['BREF','BPAR','SHAR']</content>
            <content type="fval">//</content>
            <content type="string" maxlength="25"></content>
        </Tag>
        <Tag name="22F" opt="M" repeat="true" format=":4!c/[8c]/4!c" regexp="^:[${c}]{4}\/([${c}]{0,8})?\/[${c}]{4}$">
            <content type="fval">:</content>
            <content type="enum-data" datafor="Indicator">
                {'OWNT':['NOMI','OWNE','TRUS']
                ,'BENT':['INDV','LEGL']
                ,'ADMT':['GIVE','NGIV','TIME']
                ,'RREA':['BEAR','BONU','CAPG','DVCA','EXWA','INCR','INHE','OTHR','POSS','REGD','STAF']
                ,'CERT':['NTBK','TOBK']}
            </content>
            <content type="fval">/</content>
            <content type="string" maxlength="8"></content>
            <content type="fval">/</content>
            <content type="enum" contentName="Indicator"></content>
        </Tag>
        <Tag name="98a" opt="O">
            <option>
                <Tag name="98A" format=":4!c//8!n" regexp="^:[${c}]{4}\/\/[${n}]{8}$">
                    <content type="fval">:</content>
                    <content type="enum">['RREG']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
                <Tag name="98C" format=":4!c//8!n6!n" regexp="^:[${c}]{4}\/\/[${n}]{8}[${n}]{6}$">
                    <content type="fval">:</content>
                    <content type="enum">['RREG']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
            </option>
        </Tag>
        <Tag name="35B" opt="M" format="[ISIN1!e12!c][4*35x]" regexp="^(ISIN [${c}{12}])?(([${x}]{1,35}\r\n){1,4})?$">
            <content type="string" fixedlength="12" prefix="ISIN "></content>
            <content type="textarea" cols="35" rows="4"></content>
        </Tag>
        <Tag name="70C" opt="O" format=":4!c//4*35x" regexp="^:[${c}]{4}\/\/([${x}]{1,35}\r\n){1,4}$">
            <content type="fval">:</content>
            <content type="enum">['REGI']</content>
            <content type="fval">//</content>
            <content type="textarea" cols="35" rows="4"></content>
        </Tag>
        <SubSequence name="B1" description="Optional Subsequence B1 Financial Instrument Attributes" opt="O" repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">FIA</content>
            </Tag>
            <Tag name="94B" opt="O">
                <content type="fval">:</content>
                <content type="enum">['PLIS']</content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum">['EXCH','OTCO']</content>
                <content type="string" maxlength="30" prefix="/"></content>
            </Tag>
            <Tag name="22F" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum-data" datafor="Indicator">
                    {'MICO':['A001','A002','A003','A004','A005','A006','A007','A008','A009','A010','A011','A012','A013','A014','OTHR']
                    ,'FORM':['BEAR','REGD']
                    ,'PFRE':['ANNU','MNTH','QUTR','SEMI','WEEK']
                    ,'PAYS':['FULL','NILL','PART']
                    ,'REST':['144A','NRST','RSTR']}
                </content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum" contentName="Indicator"></content>
            </Tag>
            <Tag name="12a" opt="O" repeat="true">
                <option>
                    <Tag name="12A">
                        <content type="fval">:</content>
                        <content type="enum">['OPST','OPTI']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30" ></content>
                    </Tag>
                    <Tag name="12B">
                        <content type="fval">:</content>
                        <content type="enum-data" datafor="Indicator">
                            {'OPST':['AMER','EURO']
                            ,'OPTI':['CALL','PUTO']}
                        </content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="enum" contentName="Indicator"></content>
                    </Tag>
                    <Tag name="12C">
                        <content type="fval">:</content>
                        <content type="enum">['OPST','OPTI']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="6" ></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="11A" opt="O">
                <content type="fval">:</content>
                <content type="enum">['DENO']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="3" ></content>
            </Tag>
            <Tag name="98A" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['COUP','EXPI','FRNR','MATU','ISSU','CALD','PUTT','DDTE','FCOU']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="8" ></content>
            </Tag>
            <Tag name="92A" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['PRFC','CUFC','NWFC','INTR','NXRT','YTMR']</content>
                <content type="fval">//</content>
                <content type="enum">['N']</content>
                <content type="string" maxlength="15" ></content>
            </Tag>
            <Tag name="13a" opt="O" repeat="true">
                <option>
                    <Tag name="13A">
                        <content type="fval">:</content>
                        <content type="enum">['COUP']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="3"></content>
                    </Tag>
                    <Tag name="13B">
                        <content type="fval">:</content>
                        <content type="enum">['COUP','POOL','LOTS']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="17B" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['FRNF','CALL','PUTT']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="1"></content>
            </Tag>
            <Tag name="90a" opt="O" repeat="true">
                <option>
                    <Tag name="90A">
                        <content type="fval">:</content>
                        <content type="enum">['INDC','MRKT','EXER']</content>
                        <content type="fval">//</content>
                        <content type="enum">['DISC','PRCT','PREM','YIEL']</content>
                        <content type="enum">['N']</content>
                        <content type="string" maxlength="15"></content>
                    </Tag>
                    <Tag name="90B">
                        <content type="fval">:</content>
                        <content type="enum">['INDC','MRKT','EXER']</content>
                        <content type="fval">//</content>
                        <content type="enum">['ACTU','DISC','PREM']</content>
                        <content type="string" fixedlength="3"></content>
                        <content type="string" maxlength="15"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="36B" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['MINO','SIZE']</content>
                <content type="fval">//</content>
                <content type="enum">['AMOR','FAMT','UNIT']</content>
                <content type="string" maxlength="15"></content>
            </Tag>
            <Tag name="70E" opt="O">
                <content type="fval">:</content>
                <content type="enum">['FIAN']</content>
                <content type="fval">//</content>
                <content type="textarea" cols="35" rows="10"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">FIA</content>
            </Tag>
        </SubSequence>
        <SubSequence name="B2" description="Mandatory Subsequence B2 Financial Instrument/Account" opt="M"
                     repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">FIAC</content>
            </Tag>
            <Tag name="36B" opt="M">
                <content type="fval">:</content>
                <content type="enum">['QREG']</content>
                <content type="fval">//</content>
                <content type="enum">['AMOR','FAMT','UNIT']</content>
                <content type="string" maxlength="15"></content>
            </Tag>
            <Tag name="95a" opt="O">
                <option>
                    <Tag name="95P">
                        <content type="fval">:</content>
                        <content type="enum">['ACOW']</content>
                        <content type="fval">//</content>
                        <content type="string" maxlength="11"></content>
                    </Tag>
                    <Tag name="95R">
                        <content type="fval">:</content>
                        <content type="enum">['ACOW']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="34"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="97A" opt="M">
                <content type="fval">:</content>
                <content type="enum">['SAFE']</content>
                <content type="fval">//</content>
                <content type="string" maxlength="35"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">FIAC</content>
            </Tag>
        </SubSequence>
        <Tag name="16S" opt="M">
            <content type="fval">REGDET</content>
        </Tag>
    </Sequence>
    <Sequence name="C" description="Optional Repetitive Sequence C Client Details" opt="O" repeat="true">
        <Tag name="16R" opt="M">
            <content type="fval">CLTDET</content>

        </Tag>
        <SubSequence name="C1" description="Optional Subsequence C1 Address" opt="O" repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">ADDRESS</content>
            </Tag>
            <Tag name="17B" opt="M">
                <content type="fval">:</content>
                <content type="enum">['PERM','MAIL']</content>
                <content type="fval">//Y</content>
            </Tag>
            <Tag name="22F" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum-data" datafor="Indicator">
                    {'DIST':['FIDE','PERS']
                    ,'TITL':['ASOC','DR01','FRAU','HERR','MADA','MISS','MR01','MRSS','SIRR']}
                </content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum" contentName="Indicator"></content>
            </Tag>
            <Tag name="95a" opt="O" repeat="true">
                <option>
                    <Tag name="95P">
                        <content type="fval">:</content>
                        <content type="enum">['OWND','NOMI','TRUS']</content>
                        <content type="fval">//</content>
                        <content type="string" format="IdentifierCode"></content>
                    </Tag>
                    <Tag name="95R">
                        <content type="fval">:</content>
                        <content type="enum">['OWND','NOMI','TRUS']</content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="34"></content>
                    </Tag>
                    <Tag name="95S">
                        <content type="fval">:</content>
                        <content type="enum-data" datafor="TypeofID">
                            {'ALTE':['ARNU','CCPT','CHTY','CORP','DRLC','FIIN','TXID']}
                        </content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="8"></content>
                        <content type="fval">/</content>
                        <content type="enum" contentName="TypeofID"></content>
                        <content type="fval">/</content>
                        <content type="string" fixedlength="2"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="30"></content>
                    </Tag>
                    <Tag name="95U">
                        <content type="fval">:</content>
                        <content type="enum">['OWND','NOMI','TRUS']</content>
                        <content type="fval">//</content>
                        <content type="textarea" cols="35" rows="3"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="94a" opt="O" repeat="true">
                <option>
                    <Tag name="94C">
                        <content type="fval">:</content>
                        <content type="enum">['DOMI']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="2"></content>
                    </Tag>
                    <Tag name="94D">
                        <content type="fval">:</content>
                        <content type="enum">['CITY','LOCA']</content>
                        <content type="fval">//</content>
                        <content type="string" fixedlength="2"></content>
                        <content type="fval">/</content>
                        <content type="string" maxlength="35"></content>
                    </Tag>
                    <Tag name="94G">
                        <content type="fval">:</content>
                        <content type="enum">['ADDR','EMAI']</content>
                        <content type="fval">//</content>
                        <content type="textarea" cols="35" rows="2"></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="13B" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['POST','PBOX','SNUM']</content>
                <content type="fval">//</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="string" maxlength="30"></content>
            </Tag>
            <Tag name="70C" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['REGI','PACO']</content>
                <content type="fval">//</content>
                <content type="textarea" cols="35" rows="4"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">ADDRESS</content>
            </Tag>
        </SubSequence>
        <SubSequence name="C2" description="Optional Subsequence C2 Personal Details" opt="O" repeat="false">
            <Tag name="16R" opt="M">
                <content type="fval">PERSDET</content>
            </Tag>
            <Tag name="22F" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum-data" datafor="Indicator">
                    {'OCCU':['']
                    ,'LANG':['DUTH','ENGL','FREN','GERM','ITAL','SPAN']}
                </content>
                <content type="fval">/</content>
                <content type="string" maxlength="8"></content>
                <content type="fval">/</content>
                <content type="enum" contentName="Indicator"></content>
            </Tag>
            <Tag name="95U" opt="O" >
                <content type="fval">:</content>
                <content type="enum">['BIRT']</content>
                <content type="fval">//</content>
                <content type="textarea" cols="35" rows="3"></content>
            </Tag>
            <Tag name="98a" opt="O" repeat="true">
                <option>
                    <Tag name="98A" >
                        <content type="fval">:</content>
                        <content type="enum">['DBIR','DFON']</content>
                        <content type="fval">//</content>
                        <content type="date" ></content>
                    </Tag>
                    <Tag name="98C" >
                        <content type="fval">:</content>
                        <content type="enum">['DBIR','DFON']</content>
                        <content type="fval">//</content>
                        <content type="datetime" ></content>
                    </Tag>
                </option>
            </Tag>
            <Tag name="94C" opt="O" repeat="true">
                <content type="fval">:</content>
                <content type="enum">['BIRT','NATO']</content>
                <content type="fval">//</content>
                <content type="string" fixedlength="2" ></content>
            </Tag>
            <Tag name="70C" opt="O">
                <content type="fval">:</content>
                <content type="enum">['ADTX']</content>
                <content type="fval">//</content>
                <content type="textarea" cols="35" rows="4"></content>
            </Tag>
            <Tag name="16S" opt="M">
                <content type="fval">PERSDET</content>
            </Tag>
        </SubSequence>
        <Tag name="16S" opt="M">
            <content type="fval">CLTDET</content>
        </Tag>
    </Sequence>
    <Sequence name="D" description="Optional Sequence D Corporate Action Details" opt="O" >
        <Tag name="16R" opt="M">
            <content type="fval">CADETL</content>
        </Tag>
        <Tag name="98a" opt="O" repeat="true">
            <option>
                <Tag name="98A" >
                    <content type="fval">:</content>
                    <content type="enum">['ANOU','XDTE']</content>
                    <content type="fval">//</content>
                    <content type="date" ></content>
                </Tag>
                <Tag name="98C" >
                    <content type="fval">:</content>
                    <content type="enum">['ANOU','XDTE']</content>
                    <content type="fval">//</content>
                    <content type="datetime"></content>
                </Tag>
            </option>
        </Tag>
        <Tag name="35B" opt="O">
            <content  type="string" prefix="ISIN" fixedlength="13"></content>
            <content  type="textarea" cols="35" rows="4"></content>
        </Tag>
        <Tag name="16S" opt="M">
            <content type="fval">CADETL</content>
        </Tag>
    </Sequence>
    <Sequence name="E" description="Optional Sequence E Additional Information" opt="O" >
        <Tag name="16R" opt="M">
            <content type="fval">ADDINFO</content>
        </Tag>
        <Tag name="95a" opt="O" repeat="true">
            <option>
                <Tag name="95P" >
                    <content type="fval">:</content>
                    <content type="enum">['MEOR','MERE']</content>
                    <content type="fval">//</content>
                    <content type="string" format="IdentifierCode"></content>
                </Tag>
                <Tag name="95Q" >
                    <content type="fval">:</content>
                    <content type="enum">['MEOR','MERE']</content>
                    <content type="fval">//</content>
                    <content type="textarea" cols="35" rows="4"></content>
                </Tag>
                <Tag name="95R" >
                    <content type="fval">:</content>
                    <content type="enum">['MEOR','MERE']</content>
                    <content type="fval">/</content>
                    <content type="string" maxlength="8"></content>
                    <content type="fval">/</content>
                    <content type="string" maxlength="34"></content>
                </Tag>
            </option>
        </Tag>
        <Tag name="16S" opt="M">
            <content type="fval">ADDINFO</content>
        </Tag>
    </Sequence>
</MT500>     